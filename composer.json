{
  "name": "silverstripe/cataloguepage",
  "description": "Silverstripe module that creates a simple page in the CMS to map products and categories to.",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/i-lateral/silverstripe-cataloguepage",
    "keywords": ["silverstripe", "commerce", "ecommerce", "product", "category", "catalogue", "page"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@i-lateral.com"
        }
    ],
    "support": {
        "issues": "http://github.com/i-lateral/silverstripe-catalogue/issues"
    },
    "require": {
        "silverstripe/framework": "4.*",
        "silverstripe/cms": "4.*"
    },
    "suggest": {
        "silvercommerce/catalogue-admin": "*"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "ilateral\\SilverStripe\\CataloguePage\\": "src/"
        }
    }
}
